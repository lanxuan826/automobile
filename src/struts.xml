<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC  
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"  
"http://struts.apache.org/dtds/struts-2.0.dtd">  
<struts>

	<!--设置字符集编码 -->
	<constant name="struts.i18n.encoding" value="utf8"></constant>
	<constant name="struts.objectFactory" value="spring"></constant>
	<constant name="struts.devMode" value="false" /> 

	<!-- 引入两个默认的xml -->
	<include file="struts-default.xml"></include>
	<include file="struts-plugin.xml"></include>
	
	<!-- 用户Action -->
	<include file="/struts_config/struts-user.xml"></include>
	<!-- 车辆销售Action -->
	<include file="/struts_config/struts-sale.xml"></include>
	<!-- 客户Action -->
	<include file="/struts_config/struts-customer.xml"></include>	
	<!-- 经销商Action -->
	<include file="/struts_config/struts-dealer.xml"></include>
	<!-- 仓库Action -->	
	<include file="/struts_config/struts-storge.xml"></include>
	<!--地区Action-->
	<include file="/struts_config/struts-region.xml"></include>
	<!-- 税率Action -->
	<include file="/struts_config/struts-taxrate.xml"></include>
	<!-- 车辆Action -->
	<include file="/struts_config/struts-automobile.xml"></include>
	<!-- 车型Action -->
	<include file="/struts_config/struts-autotype.xml"></include>
</struts>